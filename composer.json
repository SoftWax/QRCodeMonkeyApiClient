{
    "name": "softwax/qr-code-monkey-api-client",
    "type": "library",
    "description": "QR Code Monkey API client",
    "keywords": [
        "qrcode",
        "monkey",
        "api",
        "client"
    ],
    "homepage": "https://github.com/SoftWax/QRCodeMonkeyApiClient",
    "license": "MIT",
    "authors": [
        {
            "name": "SoftWax",
            "homepage": "https://github.com/SoftWax/QRCodeMonkeyApiClient/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "nyholm/psr7": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/httplug": "^1.0|^2.0",
        "php-http/message-factory": "^1.0",
        "php-http/multipart-stream-builder": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.0",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "SoftWax\\QRCodeMonkeyApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "SoftWax\\QRCodeMonkeyApiClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
